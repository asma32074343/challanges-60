//GND code 

#include <stdio.h>

int main()
{
    int n1,n2,gnd,i;
    printf("Please enter 2 number\n");
    scanf("%d %d",&n1,&n2);
    for( i=1;i<=n1 && i<=n2;i++){
        if(n1%i==0 && n2%i==0){
            gnd =i;
        }
        
    }
printf("the GND is %d",gnd);
    return 0;
}
///counth the number of degit
#include <stdio.h>

int main()
{
    int n1,i=1,count;
    printf("Please enter a number\n");
    scanf("%d",&n1);
    while(n1!=0){
      n1=  n1/10;
      count++;
    }
    
  
        
    
printf("the GND is %d",count);
    return 0;
}

